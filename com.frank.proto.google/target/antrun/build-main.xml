<?xml version="1.0" encoding="UTF-8" ?>
<project name="maven-antrun-" default="main"  >
<target name="main">
  <property name="protoc.filename" value="protoc-3.5.0-${os.detected.classifier}.exe"/>
  <property name="protoc.filepath" value="C:\dev_frank\com.frank.proto.tutorial\target/${protoc.filename}"/>
  <chmod file="${protoc.filepath}" perm="ugo+rx"/>
  <mkdir dir="C:\dev_frank\com.frank.proto.tutorial\target/generated-sources"/>
  <path id="protobuf.input.filepaths.path">
    <fileset dir="C:\dev_frank\com.frank.proto.tutorial/src/main/proto">
      <include name="**/*.proto"/>
    </fileset>
  </path>
  <pathconvert property="protobuf.input.filepaths" pathsep=" " refid="protobuf.input.filepaths.path"/>
  <exec failonerror="true" executable="${protoc.filepath}">
    <arg value="-I"/>
    <arg value="C:\dev_frank\com.frank.proto.tutorial/src/main/proto"/>
    <arg value="--java_out"/>
    <arg value="C:\dev_frank\com.frank.proto.tutorial\target/generated-sources"/>
    <arg line="${protobuf.input.filepaths}"/>
  </exec>
</target>
</project>